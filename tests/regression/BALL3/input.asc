TITLE BALL3 - Built from modular components via CADAC Builder
OPTIONS y_scrn n_events n_tabout y_plot n_merge y_doc n_comscrn n_traj
MODULES
	environment		def,exec
	kinematics		def,init,exec
	forces			def,exec
END
TIMING
	scrn_step 0.5
	plot_step 0.05
	int_step 0.01
END
VEHICLES 1
	BALL BALL3_TEST
		//Initial position (meters)
			sbel1  0.0    //Initial X position - m  module data
			sbel2  0.0    //Initial Y position - m  module data
			sbel3  0.0    //Initial Z position (down) - m  module data
		//Initial velocity (m/s) - 45 degree launch at 50 m/s
			vbel1  35.36    //Initial X velocity - m/s  module data
			vbel2  0.0    //Initial Y velocity - m/s  module data
			vbel3  -35.36    //Initial Z velocity (down) - m/s  module data
		//Aerodynamic properties
			cd  0.47    //Drag coefficient - ND  module data
			area  0.01    //Reference area - m^2  module data
	END
ENDTIME 10
STOP
