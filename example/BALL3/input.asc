TITLE BALL3 - Simple ballistic projectile (like a cannonball)
// Demonstrates creating a new CADAC simulation from scratch
// 3DoF point mass with gravity and drag
OPTIONS y_scrn n_events n_tabout y_plot n_merge y_doc n_comscrn n_traj
MODULES
	environment		def,exec
	kinematics		def,init,exec
	forces			def,exec
END
TIMING
	scrn_step 0.5
	plot_step 0.05
	int_step 0.01
END
VEHICLES 1
	BALL CANNONBALL
		//Initial position (meters)
			sbel1  0   //*** <<< Check spelling
			sbel2  0   //*** <<< Check spelling
			sbel3  0   //*** <<< Check spelling
		//Initial velocity (m/s) - 45 degree launch at 50 m/s
			vbel1  35.36   //*** <<< Check spelling
			vbel2  0   //*** <<< Check spelling
			vbel3  -35.36   //*** <<< Check spelling
		//Aerodynamic properties
			cd  0.47    //Drag coefficient - ND  module data
			area  0.01    //Reference area - m^2  module data
	END
ENDTIME 10
STOP
